%% Test basic model deletion functionality
\set[_echo_command="true"]

%% Create multiple models
\model-new[catalog_id="O4MC"] model-to-delete
\model-new[catalog_id="O3R"] model-to-keep
\model-new[catalog_id="O4MC"] another-model

%% Verify all models exist
\model-status

%% Delete model by exact name match
\model-delete model-to-delete

%% Verify deletion variables are set
\get[#deleted_model_name]
\get[#deleted_model_id]
\get[#deleted_model_provider]
\get[_output]

%% Verify model is actually deleted (try to activate it should fail)
\try \model-activate model-to-delete

%% Verify other models remain
\model-status

%% Verify model count decreased
\get[#model_count]