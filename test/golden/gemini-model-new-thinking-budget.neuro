%% Test Gemini model creation with various thinking_budget parameters
\set[_echo_command="true"]

%% Test Gemini Flash with dynamic thinking
\gemini-model-new[catalog_id="GM25F", thinking_budget="-1"] flash-dynamic
\get[#model_name]
\get[#model_param_count]

%% Test Gemini Flash with thinking disabled (Flash can disable)
\gemini-model-new[catalog_id="GM25F", thinking_budget="0"] flash-no-thinking
\get[#model_name]
\get[#model_param_count]

%% Test Gemini Flash with fixed thinking budget
\gemini-model-new[catalog_id="GM25F", thinking_budget="2048"] flash-fixed-thinking
\get[#model_name]
\get[#model_param_count]

%% Test Gemini Pro with dynamic thinking
\gemini-model-new[catalog_id="GM25P", thinking_budget="-1"] pro-dynamic
\get[#model_name]
\get[#model_param_count]

%% Test Gemini Pro with fixed thinking budget (within valid range)
\gemini-model-new[catalog_id="GM25P", thinking_budget="16384"] pro-fixed-thinking
\get[#model_name]
\get[#model_param_count]

%% Check final state
\model-status