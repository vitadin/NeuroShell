Setting _echo_command = true
%%> "\\set[theme_base=\"dark\"]"
Setting theme_base = dark
%%> "\\set[variant=\"compact\"]"
Setting variant = compact
%%> "\\set[suffix=\"v2\"]"
Setting suffix = v2
%%> "\\set[_style=\"${theme_base}-${variant}-${suffix}\"]"
Setting _style = dark-compact-v2
%%> "\\get[_style]"
_style = dark-compact-v2
%%> "\\set[description=\"Current theme is ${_style}\"]"
Setting description = Current theme is dark-compact-v2
%%> "\\get[description]"
description = Current theme is dark-compact-v2
%%> "\\set[user_pref=\"advanced\"]"
Setting user_pref = advanced
%%> "\\set[_style=\"${theme_base}-${user_pref}-theme\"]"
Setting _style = dark-advanced-theme
%%> "\\get[_style]"
_style = dark-advanced-theme
%%> "\\set[theme_name=\"modern\"]"
Setting theme_name = modern
%%> "\\set[style_template=\"${theme_name}-style\"]"
Setting style_template = modern-style
%%> "\\set[_style=\"${style_template}\"]"
Setting _style = modern-style
%%> "\\get[_style]"
_style = modern-style
%%> "\\get[theme_base]"
theme_base = dark
%%> "\\get[variant]"
variant = compact
%%> "\\get[_style]"
_style = modern-style