%% Test nested \try commands to verify proper error handling
\set[_echo_command="true"]
%% This test covers \try \try scenarios and complex error propagation

%% Test successful nested try
\try \try \bash echo "nested success"
\get[@status]
\get[@error]
\get[_output]

%% Test nested try with inner failure
\try \try \bash ls /nonexistent_nested
\get[@status]
\get[@error]

%% Test nested try with unknown command
\try \try \unknown_nested_command
\get[@status]
\get[@error]

%% Test triple nesting
\try \try \try \set[nested_var=deep]
\get[@status]
\get[nested_var]

%% Test nested try with outer failure (try of unknown command)
\try \unknown_outer_command \try \bash echo "never executed"
\get[@status]
\get[@error]