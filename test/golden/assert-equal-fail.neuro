%% Test assert-equal command with values that should not be equal (FAIL case)
\set[_echo_command="true"]
\set[expected_value="hello"]
\set[actual_value="world"]
\assert-equal[expect="${expected_value}", actual="${actual_value}"]
\get[_status]
\get[_assert_result]
\get[_assert_expected]
\get[_assert_actual]