%% Test \model-new delegation to \gemini-model-new for Gemini models
\set[_echo_command="true"]

%% Test Gemini Flash via \model-new (should delegate to \gemini-model-new)
\model-new[catalog_id="GM25F"] delegated-flash
\get[#model_name]
\get[#model_provider]
\get[#model_base]

%% Test Gemini Pro via \model-new with thinking_budget
\model-new[catalog_id="GM25P", thinking_budget="8192"] delegated-pro
\get[#model_name]
\get[#model_provider]
\get[#model_param_count]

%% Test Gemini Flash Lite via \model-new
\model-new[catalog_id="GM25FL", temperature="0.7", description="Delegated Flash Lite model"] delegated-lite
\get[#model_name]
\get[#model_provider]
\get[#model_description]

%% Test Gemini Flash with full parameters via \model-new delegation
\model-new[catalog_id="GM25F", temperature="0.9", thinking_budget="1024", max_tokens="1500"] delegated-full
\get[#model_name]
\get[#model_param_count]

%% Verify all models were created
\model-status