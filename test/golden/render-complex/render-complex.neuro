%% Test complex render scenarios with variable interpolation and custom output
\set[_echo_command="true"]
\set[cmd1="\\session-new"]
\set[cmd2="\\session-list"]
\render[keywords=[${cmd1}, ${cmd2}, \\help], theme=dark, style=bold, to=styled_output] Commands: ${cmd1}, ${cmd2}, and \\help are essential for session management
\get[styled_output]

%% Test with array syntax variations
\render[keywords=[ \\get , \\set ], style=highlight] Spaces in array: \\get and \\set
\get[_output]

%% Test with quoted keywords
\render[keywords=["\\bash", '\\echo'], color=#ff0000] Quoted keywords: \\bash and \\echo commands
\get[_output]

%% Test silent mode
\render[keywords=[\\vars], silent=true] This should not appear in output but be stored
\get[_output]