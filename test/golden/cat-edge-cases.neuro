%% Cat command edge cases test
\set[_echo_command="true"]
%% Tests edge cases and special content scenarios

%% Test empty file
\cat test/fixtures/empty.txt
\get[_output]

%% Test file with only newlines
\cat test/fixtures/only-newlines.txt
\get[_output]

%% Test single line without trailing newline
\cat test/fixtures/single-line.txt
\get[_output]

%% Test very long lines
\cat[path=test/fixtures/very-long-line.txt, lines=1, to=first_long_line]
\get[first_long_line]

%% Test Unicode and special characters
\cat test/fixtures/special-chars.txt
\get[_output]

%% Test with whitespace-only variable names (should work)
\cat[path=test/fixtures/simple.txt, to=test_var_123]
\get[test_var_123]

%% Test system variable assignment (should work)
\cat[path=test/fixtures/simple.txt, to=_status]
\get[_status]

%% Test with lines=1 on empty file
\cat[path=test/fixtures/empty.txt, lines=1, to=empty_with_lines]
\get[empty_with_lines]

%% Test with start=1 on empty file
\cat[path=test/fixtures/empty.txt, start=1, to=empty_with_start]
\get[empty_with_start]

%% Test newline handling - file ending with newline vs without
\cat[path=test/fixtures/simple.txt, lines=1, to=with_newline]
\get[with_newline]

\cat[path=test/fixtures/single-line.txt, to=without_newline]
\get[without_newline]

%% Test variable interpolation in path (if supported by neuro)
\set[file_path="test/fixtures/simple.txt"]
\cat[path=${file_path}, to=interpolated_path]
\get[interpolated_path]