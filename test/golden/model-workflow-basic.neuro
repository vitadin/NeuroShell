%% Test complete model management workflow
\set[_echo_command="true"]

%% Start with catalog to see available models
\model-catalog[provider="openai"]
\model-catalog[provider="openai"]

%% Create models using catalog
\model-new[catalog_id="O4MC"] my-claude-4
\model-new[catalog_id="O3R"] my-o3-model

%% Check status after creation
\model-status

%% Activate first model
\model-activate my-claude-4

%% Verify activation
\get[#active_model_name]
\get[#active_model_provider]

%% Switch to second model
\model-activate my-o3-model

%% Create additional model with custom parameters
\model-new[catalog_id="O4MC", temperature="0.8", description="Custom model"] custom-model

%% Check full status
\model-status

%% Delete one model
\model-delete my-claude-4

%% Verify deletion didn't affect other models
\model-status

%% Check that active model is still correct
\get[#active_model_name]

%% Clean up remaining models
\model-delete my-o3-model
\model-delete custom-model

%% Verify all models deleted
\model-status