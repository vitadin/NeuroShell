%% Cat command options test - individual options
\set[_echo_command="true"]
%% Tests new cat options: to, silent, lines, start

%% Test 'to' option - store in custom variable
\cat[path=test/fixtures/simple.txt, to=custom_var]
\get[custom_var]

%% Test 'silent' option - no console output, but store in _output
\cat[path=test/fixtures/simple.txt, silent=true]
\get[_output]

%% Test 'lines' option - limit to first 2 lines
\cat[path=test/fixtures/multiline.txt, lines=2]
\get[_output]

%% Test 'start' option - start from line 3
\cat[path=test/fixtures/multiline.txt, start=3]
\get[_output]

%% Test 'lines' with large number (should show all)
\cat[path=test/fixtures/simple.txt, lines=100]
\get[_output]

%% Test 'start' beyond file length (should show nothing)
\cat[path=test/fixtures/simple.txt, start=50]
\get[_output]

%% Test default behavior (no options) for comparison
\cat[path=test/fixtures/simple.txt]
\get[_output]